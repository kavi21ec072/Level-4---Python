def main():
    count=0
    
    
    for i in range(1000,10000): 
        prime=1
        for j in range(2,i):
            if(i%j==0):
               prime=0
               break
        if(prime==1 ):
            y=i
            print(f"Result = {y}")
            break
           


if __name__ == "__main__":
    main()
